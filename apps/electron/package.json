{
  "name": "in-contact-app",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Softcial",
  "description": "In Contact Electron App for Collette Health",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dist:exe": "electron-builder --win nsis",
    "dist:msi": "electron-builder --win msi",
    "dist:win": "electron-builder --win nsis,msi"
  },
  "overrides": {
    "minimatch": "3.1.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "electron-store": "^10.1.0",
    "express": "^4.19.2",
    "livekit-client": "^2.13.8",
    "react-router-dom": "^7.6.2"
  },
  "build": {
    "appId": "com.example.livekitwrapper",
    "productName": "In Contact App",
    "icon": "icon",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "assets/**/*"
    ],
    "extraResources": [
      {
        "from": ".env",
        "to": "."
      }
    ],
    "win": {
      "target": ["nsis", "msi"],
      "protocols": [
        {
          "name": "In contact app",
          "schemes": ["incontactapp"]
        }
      ]
    }
  }
}
