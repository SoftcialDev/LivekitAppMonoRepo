{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "webPubSub": {
      "connectionStringSetting": "WEBPUBSUB_CONNECTION",
      "pathPrefix": "runtime/webhooks/webpubsub"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": { "isEnabled": false }
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information",
      "Function.*": "Information",
      "Azure.ServiceBus": "Warning",
      "Azure.*": "Warning"
    }
  }
}
