{
  "name": "api-function",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean":         "rimraf dist",
    "build":          "npm run clean && tsc",
    "prepare-dist":   "cpx \"src/*/function.json\" dist && cpx \"host.json\" dist && cpx \"local.settings.json\" dist && cpx \"../../node_modules/**/*\" dist/node_modules",
    "prepare-dist:prod":"cpx \"src/*/function.json\" dist && cpx \"host.json\" dist && cpx \"../../node_modules/**/*\" dist/node_modules",
    "dev":            "npm install && npm run build && npm run prepare-dist && func start --script-root dist",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy && prisma generate"
  },
  "dependencies": {
    "@azure/functions": "^3.0.0",
    "@azure/service-bus": "^7.0.0",
    "@azure/web-pubsub": "^1.2.0",
    "@prisma/client": "^6.10.1",
    "axios": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "livekit-server-sdk": "^2.13.0",
    "module-alias": "^2.2.3",
    "qs": "^6.14.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qs": "^6.14.0",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "prisma": "^6.10.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.0"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@middleware": "dist/middleware",
    "@services": "dist/services",
    "@utils": "dist/utils"
  }
}
