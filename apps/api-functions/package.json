{
  "name": "api-function",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && tsc && npm run copy:function-json && npm run move:handlers && npm run copy:package-json && npm run copy:host-json && npm run copy:prisma",
    "clean": "rimraf dist",
    "copy:function-json": "node scripts/copy-function-json.js",
    "move:handlers": "node scripts/move-handlers-to-root.js",
    "copy:package-json": "copyfiles package.json dist",
    "copy:host-json": "copyfiles host.json dist",
    "copy:prisma": "copyfiles -u 1 \"prisma/**/*\" dist/prisma",
    "package:deploy": "node scripts/package-deploy.js",
    "build:deploy": "npm run build && npm run package:deploy",
    "prisma:generate": "prisma generate",
    "migrate:deploy": "prisma db push",
    "prepare": "npm run prisma:generate",
    "postinstall": "npm run prepare",
    "start": "func start --script-root dist",
    "seed:snapshot-reasons": "ts-node src/infrastructure/seed/seedSnapshotReasons.ts",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage"
  },
  "dependencies": {
    "@azure/functions": "^3.0.0",
    "@azure/identity": "^4.10.2",
    "@azure/service-bus": "^7.0.0",
    "@azure/storage-blob": "^12.27.0",
    "@azure/web-pubsub": "^1.2.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "livekit-server-sdk": "^2.13.0",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/microsoft-graph": "^2.40.1",
    "@types/pg": "^8.16.0",
    "@types/qs": "^6.14.0",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "jest": "^29.5.0",
    "prisma": "^7.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0"
  }
}
