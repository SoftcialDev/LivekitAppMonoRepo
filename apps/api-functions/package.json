{
  "name": "api-function",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "migrate:deploy": "prisma db push",
    "prepare": "npm run prisma:generate",
    "postinstall": "npm run prepare",
    "postbuild": "npm run prepare",
    "start": "func start --script-root .",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:cov:getUsersByRole": "jest --coverage --testPathPattern=GetUsersByRole --collectCoverageFrom=\"apps/api-functions/GetUsersByRole/index.ts\"",
    "test:cov:getSupervisorForPso": "jest --coverage --testPathPattern=GetSupervisorForPso --collectCoverageFrom=\"apps/api-functions/GetSupervisorForPso/index.ts\""
  },
  "dependencies": {
    "@azure/functions": "^3.0.0",
    "@azure/identity": "^4.10.2",
    "@azure/service-bus": "^7.0.0",
    "@azure/storage-blob": "^12.27.0",
    "@azure/web-pubsub": "^1.2.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.11.1",
    "axios": "^1.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "livekit-server-sdk": "^2.13.0",
    "module-alias": "^2.2.3",
    "qs": "^6.14.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/microsoft-graph": "^2.40.1",
    "@types/qs": "^6.14.0",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "jest": "^29.5.0",
    "prisma": "^6.11.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0"
  }
}
