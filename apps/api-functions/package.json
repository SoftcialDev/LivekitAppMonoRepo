{
  "name": "api-functions",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && npm run copy:static",
    "copy:static": "copyfiles -u 1 \"src/**/*.json\" dist && copyfiles \"host.json\" dist",
    "start": "npm run build && func start --script-root dist",
    "dev": "npm run start",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy && prisma generate"
  },
  "dependencies": {
    "@azure/functions": "^3.0.0",
    "@azure/service-bus": "^7.0.0",
    "@azure/web-pubsub": "^1.2.0",
    "@prisma/client": "^6.10.1",
    "axios": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "livekit-server-sdk": "^2.13.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "copyfiles": "^2.4.1",
    "prisma": "^6.10.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.0"
  }
}
