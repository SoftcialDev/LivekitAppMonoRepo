{
  "name": "livekitwrapper",
  "version": "1.0.0",
  "description": "",
  "author": "Softcial",
  "license": "ISC",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.3",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "@azure/msal-browser": "^4.13.2",
    "@azure/msal-react": "^3.0.13",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.9.0"
  },
  "overrides": {
    "minimatch": "10.0.1"
  },
  "build": {
    "appId": "com.example.livekitwrapper",
    "productName": "LiveKitWrapper",
    "directories": {
      "output": "dist"
    },
    "asar": true,
    "compression": "maximum",
    "npmRebuild": false,
    "files": [
      "main.js",
      "preload.js",
      "assets/**"
    ],
    "asarUnpack": [
      "**/*.node"
    ],
    "win": {
      "icon": "assets/icon.png",
      "target": [
        {
          "target": "msi",
          "arch": [ "x64", "ia32" ]
        }
      ]
    },
    "msi": {
      "perMachine": true,
      "createDesktopShortcut": true,
      "shortcutName": "LiveKitWrapper"
    }
  },
  "keywords": []
}
